<!DOCTYPE html>
<html>
  <head>
    <title>Page Title</title>

    <link rel="stylesheet" href="css/client.css">

  </head>

  <body>
    <div>
        <ul>
        <%listOfClinics.forEach( (clinic) => {%>
          <li class="clinic">
            <p class="name">
              <%=clinic.name%>
            </p>
            <p class="add">
              <%=clinic.address%>
            </p>
            <p class="tel">
              <%=clinic.open%><br/>
              <%=clinic.close%>
            </p>
          </li>
        <% }) %>
        </ul>
    </div>
  </body>

</html>
